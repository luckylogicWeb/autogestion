{
  "name": "apiautogestion-medacar",
  "version": "0.0.1",
  "description": "Autogestion Medacar",
  "main": "bin/server",
  "scripts": {
    "init:env": "cp .env.override .env",
    "start": "node bin/www/index.js",
    "build": "echo \"No build needed.\" && exit 0",
    "watch": "nodemon -L --inspect bin/www/index.js",
    "test": "jest -i",
    "test:watch": "jest -i --watch",
    "test:cov": "npm test --coverage",
    "lint": "eslint .",
    "pre-commit": "eslint .",
    "pre-push": "npm test:cov"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm pre-commit",
      "pre-push": "npm pre-push"
    }
  },
  "keywords": [],
  "author": "Medacar",
  "license": "ISC",
  "jest": {
    "displayName": {
      "name": "Boilerplate",
      "color": "blue"
    },
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "devDependencies": {
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-config-standard": "^15.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "husky": "^4.2.5",
    "jest": "^26.6.1",
    "mocked-env": "^1.3.2",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "joi": "^17.3.0",
    "nodemon": "^3.0.1",
    "oauth-1.0a": "^2.2.6",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0"
  }
}
